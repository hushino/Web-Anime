<% include ./layouts/header %> <body><h1>Vista Anime</h1><h4 class="card-title"> <%- serie.title %> </h4><br> <%- serie.synopsis.substring(0,48) %>...<br><br><h6>Tipo:</h6><ul> <%- link_to(serie.type) %> </ul><h6>Generos:</h6><ul> <% for(var i=0; i<serie.tags.length; i++) {%> <li> <%- link_to(serie.tags[i], '/'+serie.tags[i]) %> </li> <% } %> </ul><h6>Capitulos:</h6><ul> <% for(var i=0; i<serie.episodes.length; i++) {%> <li> <%- link_to(serie.episodes[i].slug, '/'+serie.episodes[i].slug) %> </li> <% }  %> </ul></body>
